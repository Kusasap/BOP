using System;
public class main{
	public static void Main(){
		Console.WriteLine(«Введите число:»);
		int s = Convert.ToInt32(Console.ReadLine());
		for(int i=1; i<s; i++){
			for(int j=1; j<s; j++){
				for(int k=1; k<s; k++){
					if(i*i+j*j==k*k){
						if(i*j/2<s){
							Console.WriteLine($«Стороны: a={i], b={j], c={k}»);}}}}}}}  
